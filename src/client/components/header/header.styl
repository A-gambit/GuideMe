.header
  width 100%
  padding 15px 100px
  border-bottom 1px solid #edeeee
  height 66px
  top 0
  position fixed
  background rgba(255,255,255, 0.9)
  z-index 4

.contact
  font-size 16px
  float right
  text-decoration none
  padding 5px 20px
  border 1px solid #56565
  transition 0.2s
  &:hover
    color #fff
    transition 0.2s
    background-color #565656

.logo
  font-size 25px
  font-family 'Pacifico'
  text-decoration none
  position absolute
  margin-top -5px
  color #565656

